################################# ped: Building definitions ###############
###  http://example.org/ontology#PostalAddress
ped:PostalAddress rdf:type owl:Class ;
    rdfs:label "Postal address"^^xsd:string ;
    rdfs:comment "Physical address of the architecture (site, building, sub-building, entrance room, etc.)";
    rdfs:subClassOf [
                    rdf:type owl:Restriction ; 
                    owl:onProperty ped:addressLine1 ;
                    owl:cardinality "1"^^xsd:nonNegativeInteger ;
                    owl:onClass xsd:string ;
                   ],
                   [
                    rdf:type owl:Restriction ; 
                    owl:onProperty ped:addressLine2 ;
                    owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                    owl:onClass xsd:string ;
                   ],
                   [
                    rdf:type owl:Restriction ; 
                    owl:onProperty ped:City ;
                    owl:cardinality "1"^^xsd:nonNegativeInteger ;
                    owl:onClass xsd:string ;
                   ],
                    [
                    rdf:type owl:Restriction ; 
                    owl:onProperty ped:Country ;
                    owl:cardinality "1"^^xsd:nonNegativeInteger ;
                    owl:onClass xsd:string ;
                   ] ,
                   [
                    rdf:type owl:Restriction ; 
                    owl:onProperty ped:postalCode ;
                    owl:cardinality "1"^^xsd:nonNegativeInteger ;
                    owl:onClass xsd:string ;
                   ] .

###  http://example.org/ontology#hasPostalAddress                   
ped:hasPostalAddress rdf:type owl:ObjectProperty;
   rdfs:label "hasPostalAddress"@en ;
   rdfs:domain ped:Building;
   rdfs:range  ped:PostalAddress  .


###  http://example.org/ontology#addressLine1
ped:addressLine1 rdf:type owl:DatatypeProperty;
    rdfs:label "addressLine1"@en ;
    rdfs:domain ped:PostalAddress ;
    rdfs:range xsd:string .

###  http://example.org/ontology#addressLine2
ped:addressLine2 rdf:type owl:DatatypeProperty ;
    rdfs:label "addressLine2"@en ;
    rdfs:domain ped:PostalAddress ;
    rdfs:range xsd:string .

###  http://example.org/ontology#postalCode
ped:postalCode rdf:type owl:DatatypeProperty ;
    rdfs:label "postalCode"@en ;
    rdfs:domain ped:PostalAddress ;
    rdfs:range xsd:string  .

###  http://example.org/ontology#City
ped:City rdf:type owl:DatatypeProperty ;
    rdfs:label "City"@en ;
    rdfs:domain ped:PostalAddress ;
    rdfs:range xsd:string .

###  http://example.org/ontology#Country
ped:Country rdf:type owl:DatatypeProperty ;
    rdfs:label "Country"@en ;
    rdfs:domain ped:PostalAddress ;
    rdfs:range xsd:string  .

###  http://example.org/ontology#yearBuilt
ped:yearBuilt rdf:type owl:DatatypeProperty ;
    rdfs:label "Year built"@en ;
    rdfs:domain ped:Building ;
    rdfs:range [
        a rdfs:Datatype ;
        owl:onDatatype xsd:integer ;
        owl:withRestrictions ( 
            [ xsd:minInclusive "1000"^^xsd:integer ]
            [ xsd:maxInclusive "9999"^^xsd:integer ]
        )
    ] ;
    rdfs:comment "Four-digit year that a building was first built." .

###  http://example.org/culturalClassification
ped:culturalClassification a owl:DatatypeProperty ;
    rdfs:label "Cultural Classification"@en ;
    rdfs:domain ped:Building ;
    rdfs:range [
        a rdfs:Datatype ;
        owl:onDatatype xsd:string ;
        owl:oneOf (
            "Archaeological"
            "Historical"
            "Architectural"
            "Religious"
            "Modernist"
            "Heritage Site"
            "UNESCO World Heritage"
        )
    ] ;
    rdfs:comment "The cultural classification of a building, chosen from a predefined list."@en .

### Define the Coordinates class
ped:Coordinates a owl:Class ;
    rdfs:label "Coordinates"@en ;
    rdfs:comment "A class representing geographic coordinates (latitude and longitude)."@en .

### Define latitude property
ped:latitude a owl:DatatypeProperty ;
    rdfs:label "Latitude"@en ;
    rdfs:domain ped:Coordinates ;
    rdfs:range xsd:double ;
    rdfs:comment "Latitude of the building location."@en .

### Define longitude property
ped:longitude a owl:DatatypeProperty ;
    rdfs:label "Longitude"@en ;
    rdfs:domain ped:Coordinates ;
    rdfs:range xsd:double ;
    rdfs:comment "Longitude of the building location."@en .

### Define the hasCoordinates property
ped:hasCoordinates a owl:ObjectProperty ;
    rdfs:label "Has Coordinates"@en ;
    rdfs:domain ped:Building ;
    rdfs:range ped:Coordinates ;
    rdfs:comment "Links a building to its geographic coordinates."@en .

########### example ########
ped:NotreDame a ped:Building ;
    rdfs:label "Notre Dame Cathedral"@en ;
    ped:hasCoordinates ped:NotreDameCoords .

ped:NotreDameCoords a ped:Coordinates ;
    ped:latitude 48.852968 ;
    ped:longitude 2.349902 .

