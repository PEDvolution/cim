#    Object Properties
#################################################################

###  http://example.org/ontology#aggregatedBuildingConstruction
ener_ade:aggregatedBuildingConstruction rdf:type owl:ObjectProperty ;
                                        rdfs:subPropertyOf owl:topObjectProperty ;
                                        rdfs:domain ener_ade:AbstractBuilding ;
                                        rdfs:range ener_ade:AbstractConstruction ;
                                        rdfs:comment "Association of a complete building with \"average\" material properties"@en .


###  http://example.org/ontology#averageInternalGains
ener_ade:averageInternalGains rdf:type owl:ObjectProperty ;
                              rdfs:domain ener_ade:UsageZone ;
                              rdfs:range ener_ade:HeatExchangeType ;
                              rdfs:comment "Aggregated average heat energy per year dissipated by the occupants and facilities in a usage zone over the whole year (positive for heat source, negative for heat sink/cool source)."@en .


###  http://example.org/ontology#boundedBy
ener_ade:boundedBy rdf:type owl:ObjectProperty ;
                   rdfs:domain ener_ade:ThermalZone ;
                   rdfs:range ener_ade:ThermalBoundary ;
                   rdfs:comment "Reference of a ThermalZone to it's bounding ThermalBoundary objects."@en .


###  http://example.org/ontology#buildingType
ener_ade:buildingType rdf:type owl:ObjectProperty ;
                      rdfs:domain ener_ade:AbstractBuilding ;
                      rdfs:range ener_ade:BuildingTypeValue .


###  http://example.org/ontology#construction
ener_ade:construction rdf:type owl:ObjectProperty ;
                      rdfs:domain ener_ade:ThermalBoundary ,
                                  ener_ade:ThermalOpening ;
                      rdfs:range ener_ade:AbstractConstruction ;
                      rdfs:comment "Association of material parameters with the opaque part of the thermal boundary."@en .


###  http://example.org/ontology#constructionWeight
ener_ade:constructionWeight rdf:type owl:ObjectProperty ;
                            rdfs:domain ener_ade:AbstractBuilding ;
                            rdfs:range ener_ade:ConstractionWeightValue ;
                            rdfs:comment "Classification of building according with construction structure."@en .


###  http://example.org/ontology#contains
ener_ade:contains rdf:type owl:ObjectProperty ;
                  rdfs:domain ener_ade:UsageZone ;
                  rdfs:range ener_ade:BuildingUnit ;
                  rdfs:comment "Association between UsageZone class and BuildingUnit class."@en .


###  http://example.org/ontology#coolingSchedule
ener_ade:coolingSchedule rdf:type owl:ObjectProperty ;
                         rdfs:domain ener_ade:UsageZone ;
                         rdfs:range ener_ade:AbstructSchedule ;
                         rdfs:comment "Temperature schedule of the active cooling inside the usage zone."@en .


###  http://example.org/ontology#delimits
ener_ade:delimits rdf:type owl:ObjectProperty ;
                  rdfs:domain ener_ade:ThermalBoundary ;
                  rdfs:range ener_ade:ThermalZone ;
                  rdfs:comment """In case two ThermalZones are referenced via the relation \"delimits\", the following rule concerning the order of the related objects applys:

- For ThermalBoundary objects of type \"intermediaryFloor\", the first related object is the upper ThermalZone, and the second related object is the lower ThermalZone.
- For all other ThermalBoundary types, the first related ThermalZone is in contact with the last Layer of the corresponding AbstractConstruction, and the second related ThermalZone is in contact with the first Layer of the corresponding AbstractConstruction."""@en .


